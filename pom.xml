    <groupId>io.jenkins.plugins</groupId>
    <artifactId>login-logo</artifactId>
    <version>1.0.3-SNAPSHOT</version>
    <packaging>hpi</packaging>
    <description>This Jenkins plugin adds a custom logo to your Jenkins login page. It also allows to replace Welcome Jenkins message and add Extra CSS.</description>
    <properties>
        <jenkins.version>2.164.1</jenkins.version>
        <java.level>8</java.level>
    </properties>
    <name>Login Logo</name>
    <licenses>
        <license>
            <name>MIT License</name>
            <url>https://opensource.org/licenses/MIT</url>
        </license>
    </licenses>

    <developers>
        <developer>
            <name>Alvaro Saez de Guinoa</name>
            <email>alvaro1972@gmail.com</email>
            <organization>Alvaro Saez de Guinoa</organization>
            <organizationUrl>https://github.com/alvaro72</organizationUrl>
        </developer>
    </developers>

    <scm>
        <connection>scm:git:git://github.com/alvaro72/loginlogo.git</connection>
        <developerConnection>scm:git:ssh://github.com:alvaro72/loginlogo.git</developerConnection>
        <url>https://github.com/alvaro72/loginlogo</url>
    </scm>

    <repositories>
        <repository>
            <id>repo.jenkins-ci.org</id>
            <url>https://repo.jenkins-ci.org/public/</url>
        </repository>
    </repositories>
    <pluginRepositories>
        <pluginRepository>
            <id>repo.jenkins-ci.org</id>
            <url>https://repo.jenkins-ci.org/public/</url>
        </pluginRepository>
    </pluginRepositories>
</project>
